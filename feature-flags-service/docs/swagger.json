            ],
            "properties": {
                "feature_key": {
                    "type": "string"
                },
                "value": {}
            }
        },
        "handlers.TrackExperimentRequest": {
            "type": "object",
            "required": [
                "variation_id"
            ],
            "properties": {
                "attributes": {
                    "type": "object",
                    "additionalProperties": true
                },
                "user_id": {
                    "type": "string"
                },
                "variation_id": {
                    "type": "string"
                }
            }
