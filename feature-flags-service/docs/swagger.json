    "paths": {
        "/experiments": {
            "get": {
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "description": "Get all experiments for the tenant",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Experiments"
                ],
                "summary": "List experiments",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Tenant/Vendor ID",
                        "name": "X-Vendor-ID",
                        "in": "header",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "List of experiments",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        },
        "/experiments/{id}": {
            "get": {
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "description": "Get a specific experiment by ID",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Experiments"
                ],
                "summary": "Get experiment",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Tenant/Vendor ID",
                        "name": "X-Vendor-ID",
                        "in": "header",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Experiment ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Experiment details",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "404": {
                        "description": "Experiment not found",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        },
        "/experiments/{id}/track": {
            "post": {
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "description": "Track an experiment view or conversion event",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Experiments"
                ],
                "summary": "Track experiment",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Tenant/Vendor ID",
                        "name": "X-Vendor-ID",
                        "in": "header",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Experiment ID",
                        "name": "id",
                        "in": "path",
                        "required": true
                    },
                    {
                        "description": "Tracking request",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/handlers.TrackExperimentRequest"
