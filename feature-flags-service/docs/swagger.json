    "definitions": {
        "handlers.BatchEvaluateRequest": {
            "type": "object",
            "required": [
                "client_key",
                "feature_keys"
            ],
            "properties": {
                "attributes": {
                    "type": "object",
                    "additionalProperties": true
                },
                "client_key": {
                    "type": "string"
                },
                "feature_keys": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            }
        },
        "handlers.EvaluateFeatureRequest": {
            "type": "object",
            "required": [
                "client_key",
                "feature_key"
            ],
            "properties": {
                "attributes": {
                    "type": "object",
                    "additionalProperties": true
                },
                "client_key": {
                    "type": "string"
                },
                "feature_key": {
                    "type": "string"
                }
            }
        },
        "handlers.OverrideRequest": {
            "type": "object",
            "required": [
                "feature_key",
